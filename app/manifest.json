{
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "version": "1.0.0",
  "manifest_version": 2,
  "default_locale": "ja",
  "icons": {
    "128": "images/icon-128.png"
  },
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["scripts/content.js"],
    "run_at": "document_end",
    "all_frames": false
  }],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "storage"
  ]
}
